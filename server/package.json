{
	"name": "server",
	"version": "1.0.0",
	"description": "",
	"main": "index.ts",
	"scripts": {
		"start": "ts-node -r tsconfig-paths/register src/index.ts",
		"dev": "nodemon -r tsconfig-paths/register --exec ts-node src/index.ts",
		"dev:test": "DB_HOST=127.0.0.1 DB_PORT=5445 DB_NAME=test_postgres DB_USERNAME=postgres DB_PASSWORD=mysecretpassword nodemon -r tsconfig-paths/register --exec ts-node src/index.ts",
		"build": "tsc",
		"test": "jest --coverage",
		"test:ci": "npm run db:test:start && npm test -- --runInBand && npm run db:test:stop",
		"db:test:start": "docker compose -f ./test/docker/postgres/docker-compose.yaml up -d",
		"db:test:stop": "docker compose -f ./test/docker/postgres/docker-compose.yaml down",
		"db:dev:start": "docker compose -f ./docker/postgres/docker-compose.yaml up -d",
		"db:dev:stop": "docker compose -f ./docker/postgres/docker-compose.yaml down",
		"populate-db": "ts-node -r tsconfig-paths/register scripts/populate-db.ts",
		"test:unit": "jest --testPathPatterns=test/unit --runInBand",
		"test:e2e": "jest --testPathPatterns=test/e2e --runInBand"
	},
	"keywords": [],
	"type": "commonjs",
	"dependencies": {
		"@turf/helpers": "^7.2.0",
		"@turf/turf": "^7.2.0",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.5",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.9.2",
		"@types/supertest": "^6.0.3",
		"@types/swagger-ui-express": "^4.1.8",
		"@types/yamljs": "^0.2.34",
		"bcryptjs": "^3.0.3",
		"class-validator": "^0.14.2",
		"cors": "^2.8.5",
		"date-fns": "^4.1.0",
		"dotenv": "^17.2.3",
		"express": "^5.1.0",
		"express-openapi-validator": "^5.6.0",
		"form-data": "^4.0.4",
		"jest": "^30.2.0",
		"jest-junit": "^16.0.0",
		"jsonwebtoken": "^9.0.2",
		"nodemon": "^3.1.10",
		"pg": "^8.16.3",
		"prettier": "^3.6.2",
		"reflect-metadata": "^0.2.2",
		"supertest": "^7.1.4",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"telegraf": "^4.16.3",
		"telegraf-session-local": "^2.1.1",
		"ts-jest": "^29.4.5",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"typeorm": "^0.3.27",
		"typescript": "^5.9.3",
		"winston": "^3.18.3",
		"yamljs": "^0.3.0"
	},
	"devDependencies": {
		"@types/geojson": "^7946.0.16",
		"@types/jsonwebtoken": "^9.0.10"
	}
}
